server {

    server_name pictureonmap.com www.pictureonmap.com;

    location /static/ {
        root /home/pictureonmap/Project;
    }

    location /media/ {
        root /home/pictureonmap/Project;
    }

    location / {
        include proxy_params;
        proxy_pass http://127.0.0.1:8000;
    }

}

## nginx.conf

client_max_body_size 120m;
